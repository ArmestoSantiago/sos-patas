      ],
      "@/services": [
        "src/services/*"
      ],
      "@/types": [
        "src/types/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/hooks": [
        "src/hooks/*"
      ]
    }
  }
}